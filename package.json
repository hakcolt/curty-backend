{
  "name": "curty-backend",
  "version": "1.0.0",
  "description": "Backend to Curty URL Shortner",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf dist/**/* && tsc --sourceMap false",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:dev": "prisma migrate dev",
    "start": "NODE_ENV=production node ./dist/index.js"
  },
  "repository": "https://github.com/hakcolt/ts-template.git",
  "author": "Igor Hakcolt",
  "keywords": [
    "node",
    "express",
    "clean-architecture",
    "solid",
    "ddd",
    "multi-language",
    "advanced-api"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.16",
    "@types/uuid": "^9.0.0",
    "@vitest/coverage-c8": "^0.26.3",
    "prisma": "^4.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4",
    "vitest": "^0.26.2"
  },
  "dependencies": {
    "@hakcolt/resources": "^1.1.0",
    "@hakcolt/result": "^1.2.0",
    "@hakcolt/validator": "^1.0.0",
    "@prisma/client": "^4.9.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "uuid": "^9.0.0"
  }
}
